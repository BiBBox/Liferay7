<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 7.0.0//EN" "http://www.liferay.com/dtd/liferay-service-builder_7_0_0.dtd">
<service-builder package-path="at.graz.meduni.bibbox.liferay.portlet">
	<author>Robert Reihs</author>
	<namespace>bibboxdocker</namespace>
	
	<entity name="ApplicationStoreItem" local-service="true" remote-service="true">
		<!-- PK fields -->
		<column name="applicationStoreItemId" type="long" primary="true" />
		
		<!-- Group instance -->
		<column name="groupId" type="long" />

		<!-- Audit fields -->
		<column name="companyId" type="long" />
		<column name="userId" type="long" />
		<column name="userName" type="String" />
		<column name="createDate" type="Date" />
		<column name="modifiedDate" type="Date" />
		<column name="githash" type="String" />
		<column name="depreciated" type="Boolean" />
		
		<!-- Other fields -->
		<column name="applicationFolderName" type="String" />
		<column name="applicationName" type="String" />
		<column name="applicationShortName" type="String" />
		<column name="applicationVersion" type="String" />
		<column name="applicationReleaseState" type="String" />
		<column name="applicationDescription" type="String" />
		<column name="applicationCatalogueUrl" type="String" />
		<column name="applicationApplicationUrl" type="String" />
		
		<!-- Order -->
		<order by="asc">
			<order-column name="applicationName" />
		</order>
		
		<!-- Finder methods -->
		<finder name="ApplicationName" return-type="ApplicationStoreItem">
			<finder-column name="applicationName" />
		</finder>
	</entity>
	
	<entity name="KitGroup" local-service="true" remote-service="true">
		<!-- PK fields -->
		<column name="kitGroupId" type="long" primary="true" />
		
		<!-- Group instance -->
		<column name="groupId" type="long" />

		<!-- Audit fields -->
		<column name="companyId" type="long" />
		<column name="userId" type="long" />
		<column name="userName" type="String" />
		<column name="createDate" type="Date" />
		<column name="modifiedDate" type="Date" />
		
		<!-- Other fields -->
		<column name="group" type="String" />
		<column name="applicationStoreItemId" type="long" />
		<column name="bibboxKitId" type="long" />
		
		<!-- Order -->
		<order by="asc">
			<order-column name="group" />
		</order>
		
		<!-- Finder methods -->
		<finder name="KitGroup" return-type="Collection">
			<finder-column name="applicationStoreItemId" />
			<finder-column name="bibboxKitId" />
		</finder>
		
		<finder name="KitGroups" return-type="Collection">
			<finder-column name="applicationStoreItemId" />
		</finder>
		
	</entity>
	
	<entity name="ApplicationTag" local-service="true" remote-service="true">
		<!-- PK fields -->
		<column name="applicationTagsId" type="long" primary="true" />
		
		<!-- Group instance -->
		<column name="groupId" type="long" />

		<!-- Audit fields -->
		<column name="companyId" type="long" />
		<column name="userId" type="long" />
		<column name="userName" type="String" />
		<column name="createDate" type="Date" />
		<column name="modifiedDate" type="Date" />
		
		<!-- Other fields -->
		<column name="tag" type="String" />
		<column name="applicationStoreItemId" type="long" />
		
		<!-- Order -->
		<order by="asc">
			<order-column name="tag" />
		</order>

		<!-- Finder methods -->
		<finder name="ApplicationStoreItemId" return-type="Collection">
			<finder-column name="applicationStoreItemId" />
		</finder>
		<finder name="Tag" return-type="Collection">
			<finder-column name="tag" />
		</finder>
	</entity>
	
	<entity name="BibboxKit" local-service="true" remote-service="true">
		<!-- PK fields -->
		<column name="bibboxKitId" type="long" primary="true" />
		
		<!-- Group instance -->
		<column name="groupId" type="long" />

		<!-- Audit fields -->
		<column name="companyId" type="long" />
		<column name="userId" type="long" />
		<column name="userName" type="String" />
		<column name="createDate" type="Date" />
		<column name="modifiedDate" type="Date" />
		
		<!-- Other fields -->
		<column name="kitName" type="String" />
		<column name="applicationStoreItemId" type="long" />
		
		<!-- Order -->
		<order by="asc">
			<order-column name="kitName" />
		</order>

		<!-- Finder methods -->
		<finder name="ApplicationStoreItemId" return-type="Collection">
			<finder-column name="applicationStoreItemId" />
		</finder>
		<finder name="KitName" return-type="Collection">
			<finder-column name="kitName" />
		</finder>
		<finder name="KitName" return-type="BibboxKit">
			<finder-column name="kitName" />
			<finder-column name="applicationStoreItemId" />
		</finder>
	</entity>

	<entity name="DockerContainer" local-service="true" remote-service="true">

		<!-- PK fields -->
		<column name="dockerContainerId" type="long" primary="true" />

		<!-- Group instance -->
		<column name="groupId" type="long" />

		<!-- Audit fields -->
		<column name="companyId" type="long" />
		<column name="userId" type="long" />
		<column name="userName" type="String" />
		<column name="createDate" type="Date" />
		<column name="modifiedDate" type="Date" />

		<!-- Other fields -->
		<column name="startCommand" type="String" />
		<column name="toolName" type="String" />
		<column name="instance" type="String" />
		<column name="port" type="String" />
		<column name="url" type="String" />
		<column name="subdomain" type="String" />
		<column name="ip" type="String" />
		
		<column name="displayName" type="String" />
		<column name="displayToolName" type="String" />
		<column name="category" type="String" />
		<column name="tool" type="String" />
		<column name="description" type="String" />
		<column name="company" type="String" />
		<column name="platform" type="String" />
		<column name="license" type="String" />
		<column name="toolDescription" type="String" />
		<column name="logourl" type="String" />
		

		<!-- Order -->
		<order by="asc">
			<order-column name="dockerContainerId" />
		</order>

		<!-- Finder methods -->
		<finder name="DockerContainer" return-type="DockerContainer">
			<finder-column name="toolName" />
			<finder-column name="instance" />
		</finder>
		<finder name="DockerContainerInstances" return-type="Collection">
			<finder-column name="toolName" />
		</finder>
	</entity>
</service-builder>