<?xml version="1.0"?>
<!DOCTYPE look-and-feel PUBLIC "-//Liferay//DTD Look and Feel 7.0.0//EN" "http://www.liferay.com/dtd/liferay-look-and-feel_7_0_0.dtd">

<look-and-feel>
	<compatibility>
		<version>7.0.0+</version>
	</compatibility>
	<theme id="bibbox" name="BIBBOX">
		<template-extension>ftl</template-extension>
		<portlet-decorator id="barebone" name="Barebone">
			<default-portlet-decorator>true</default-portlet-decorator>
			<portlet-decorator-css-class>portlet-barebone</portlet-decorator-css-class>
		</portlet-decorator>
		<portlet-decorator id="borderless" name="Borderless">
			<portlet-decorator-css-class>portlet-borderless</portlet-decorator-css-class>
		</portlet-decorator>
		<portlet-decorator id="decorate" name="Decorate">
			<default-portlet-decorator>false</default-portlet-decorator>
			<portlet-decorator-css-class>portlet-decorate</portlet-decorator-css-class>
		</portlet-decorator>
		<portlet-decorator id="title-two-column" name="Columns with title">
			<default-portlet-decorator>false</default-portlet-decorator>
			<portlet-decorator-css-class>portlet-borderless two-column</portlet-decorator-css-class>
		</portlet-decorator>
		<portlet-decorator id="title-formatted-columns" name="Formatted Columns with title">
			<default-portlet-decorator>false</default-portlet-decorator>
			<portlet-decorator-css-class>portlet-borderless two-column formatted</portlet-decorator-css-class>
		</portlet-decorator>
		<portlet-decorator id="title-formatted" name="Formatted with title">
			<default-portlet-decorator>false</default-portlet-decorator>
			<portlet-decorator-css-class>portlet-borderless formatted</portlet-decorator-css-class>
		</portlet-decorator>
		<portlet-decorator id="two-column" name="Columns without title">
			<default-portlet-decorator>false</default-portlet-decorator>
			<portlet-decorator-css-class>portlet-barebone two-column</portlet-decorator-css-class>
		</portlet-decorator>
		<portlet-decorator id="formatted-columns" name="Formatted Columns without title">
			<default-portlet-decorator>false</default-portlet-decorator>
			<portlet-decorator-css-class>portlet-barebone two-column formatted</portlet-decorator-css-class>
		</portlet-decorator>
		<portlet-decorator id="formatted" name="Formatted without title">
			<default-portlet-decorator>false</default-portlet-decorator>
			<portlet-decorator-css-class>portlet-barebone formatted</portlet-decorator-css-class>
		</portlet-decorator>
	</theme>
</look-and-feel>
